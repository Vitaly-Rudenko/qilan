{
  "name": "qilan",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "test": "jasmine --config=spec/support/jasmine.json",
    "test:watch": "nodemon -x 'npm test'"
  },
  "devDependencies": {
    "jasmine": "3.6.3"
  },
  "author": "Vitaly Rudenko",
  "description": "Smart JavaScript mocking framework",
  "license": "GPL-3.0-only",
  "keywords": [
    "javascript",
    "mock",
    "stub",
    "unit-testing",
    "test"
  ],
  "homepage": "https://github.com/vitaly-rudenko/qilan#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitaly-rudenko/qilan.git"
  },
  "bugs": {
    "url": "https://github.com/vitaly-rudenko/qilan/issues"
  },
  "dependencies": {
    "nodemon": "2.0.7"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules",
      "package.json"
    ],
    "delay": "500",
    "legacyWatch": false,
    "signal": "SIGTERM",
    "ext": "js"
  }
}
